        <div class="d-none">
          <asp:Label ID="ResultLB" runat="server"></asp:Label>
          <asp:Label ID="LoginLB" runat="server"></asp:Label>
        </div>
        <div class="rd-navbar-wrap">
          <nav class="rd-navbar">
            <div class="rd-navbar-panel">
              <div class="rd-navbar-panel-cell">
                <button class="btn button-navbar" data-navigation-switch="data-navigation-switch"><span
                    class="fa-bars"></span></button>
              </div>
              <div class="rd-navbar-panel-cell d-none d-xl-block">
                <!-- Breadcrumbs-->
                <ul class="breadcrumbs">
                  <li class="breadcrumbs-item">
                    <a class="breadcrumbs-link" href="order.aspx"><span
                        class="breadcrumbs-icon mdi mdi-view-grid"></span><span>總覽頁面</span></a></li>
                </ul>
              </div>
              <div class="rd-navbar-panel-center"></div>
  
              <div class="rd-navbar-panel-cell">
                <button class="btn button-navbar"
                  data-multi-switch='{"targets":"#subpanel-notifications","scope":"#subpanel-notifications","isolate":"[data-multi-switch]"}'><span
                    class="fa-bell"></span><span class="badge badge-success rounded-circle">2</span></button>
                <div class="rd-navbar-subpanel" id="subpanel-notifications">
                  <div class="panel panel-sm">
                    <div class="panel-header text-dark">
                      <h5><span class="panel-icon fa-bell"> </span></h5>
                    </div>
                    <div class="panel-body scroller scroller-vertical">
                      <div class="group-5">
                        <div class="alert alert-dismissible alert-primary" role="alert">
                          <span class="alert-icon fa-thumbs-up"></span>
                          <span>退休金準備計畫已下單</span>
                          <button class="close" type="button" data-dismiss="alert" aria-label="Close">
                            <span class="fa-close" aria-hidden="true"></span>
                          </button>
                        </div>
                        <div class="alert alert-dismissible alert-info" role="alert">
                          <span class="alert-icon fa-telegram"></span>
                          <span>12/30將進行再平衡</span>
                          <button class="close" type="button" data-dismiss="alert" aria-label="Close">
                            <span class="fa-close" aria-hidden="true"></span>
                          </button>
                        </div>
                        <div class="alert alert-dismissible alert-secondary" role="alert">
                          <span class="alert-icon fa-warning"></span>
                          <span>子女教育金計畫下單失敗</span>
                          <button class="close" type="button" data-dismiss="alert" aria-label="Close">
                            <span class="fa-close" aria-hidden="true"></span>
                          </button>
                        </div>
                        <div class="alert alert-dismissible alert-success" role="alert">
                          <span class="alert-icon fa-thumbs-up"></span>
                          <span>您的退休金計畫加碼成功</span>
                          <button class="close" type="button" data-dismiss="alert" aria-label="Close">
                            <span class="fa-close" aria-hidden="true"></span>
                          </button>
                        </div>
                      </div>
                    </div>
                    <div class="panel-footer"><a class="btn btn-dark btn-sm">說明</a></div>
                  </div>
                </div>
              </div>
              <div class="rd-navbar-panel-cell">
