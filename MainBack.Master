<%@ Master Language="VB" AutoEventWireup="false" CodeBehind="MainBack.master.vb" Inherits="Financial_Robots.MainBack" %>

<!DOCTYPE html>

<html class="root rd-navbar-sidebar-active page-small-footer" lang="zh-TW" xmlns="http://www.w3.org/1999/xhtml">

<head runat="server">
  <title>Dashboard</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta property="og:title" content="Template Monster Admin Template">
